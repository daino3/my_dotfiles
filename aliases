# Unix

alias b='bundle exec'
alias subl='sublime'
alias cdwp='cd /Applications/MAMP/htdocs'

# Zeus preloader
alias zi='zeus init'
alias zs='zeus start'
alias zt='zeus test'
alias zr='zeus rake'

# Easy common file navigation
alias cdtda='cd ~/Programs/tastytrade/tda_trade_lab'
alias cdpm='cd ~/Programs/tastytrade/Podcast-Manager'
alias cdtt='cd ~/Programs/tastytrade/tastytrade_v2'
alias cdauth='cd ~/Programs/tastytrade/dough-unified-credentials'
alias cdlib='cd ~/Programs/tastytrade/tastytrade-lib'
alias cdsp='cd ~/Programs/stock_picker'
alias cdfw='cd ~/Programs/tastytrade/filming-waivers'
alias cdttios='cd ~/Programs/tastytrade/tastytrade_ios'
alias cdyf="cd ~/Programs/yahoo-finance"
alias cdblog="cd ~/Programs/blogger"
alias cdbayes="cd ~/Programs/Probabilistic-Programming-and-Bayesian-Methods-for-Hackers"
alias cdts="cd ~/Programs/tastytrade/trade_stalker"
alias cdttr="cd ~/Programs/tastytrade/tastytrade-responsive"

# Git
alias ga='git add'
alias gst='git status'
alias gc='git commit'
alias gb='git branch'
alias gl='git log'
alias gco='git checkout'
alias gpull='VAR=`git rev-parse --abbrev-ref HEAD`; git pull origin $VAR'
alias gpullr='git pull --rebase origin master'
alias gpush='VAR=`git rev-parse --abbrev-ref HEAD`; git push origin $VAR'

# kill xcode (awesome when it hangs)
alias killxc="kill $(pgrep 'Xcode')"

# Rails
alias m="migrate"
alias rk="rake"
alias s="rspec"

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local

# Vagrant
alias vssh="vagrant ssh"

# Alias vim command to use system ruby for YouCompleteMe
# https://github.com/Valloric/YouCompleteMe/issues/151
alias vim='rvm system do /usr/local/bin/vim $@'

